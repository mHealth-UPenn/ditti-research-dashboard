{
    "app": {
        "app_function": "run.app",
        "aws_region": "{{ AWS_REGION }}",
        "environment_variables": {
            "FLASK_CONFIG": "Production",
            "AWS_SECRET_NAME": "{{ AWS_PRODUCTION_SECRET_NAME }}"
        },
        "events": [{
            "function": "rds_stopper.stop",
            "expression": "rate(1 hour)"
        }],
        "log_level": "INFO",
        "project_name": "{{ PROJECT_NAME }}",
        "runtime": "python3.12",
        "tags": {
            "{{ TAG_KEY }}": "{{ TAG_VALUE }}"
        },
        "touch": false,
        "certificate_arn": "{{ CERTIFICATE_ARN }}",
        "domain": "{{ PRODUCTION_DOMAIN }}"
    },
    "staging": {
        "app_function": "run.app",
        "aws_region": "{{ AWS_REGION }}",
        "environment_variables": {
            "FLASK_CONFIG": "Production",
            "AWS_SECRET_NAME": "{{ AWS_STAGING_SECRET_NAME }}"
        },
        "events": [{
            "function": "rds_stopper.stop",
            "expression": "rate(1 hour)"
        }],
        "log_level": "INFO",
        "project_name": "{{ PROJECT_NAME }}",
        "runtime": "python3.12",
        "tags": {
            "{{ TAG_KEY }}": "{{ TAG_VALUE }}"
        },
        "touch": false,
        "certificate_arn": "{{ CERTIFICATE_ARN }}",
        "domain": "{{ STAGING_DOMAIN }}"
    }
}
